{
  "name": "my_nextjs_template",
  "version": "0.1.0",
  "private": true,
  "type-check": "tsc -b",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "test": "vitest --reporter=verbose"
  },
  "dependencies": {
    "next": "15.3.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
